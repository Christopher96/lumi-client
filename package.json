{
  "name": "lumi-cli",
  "version": "0.0.26",
  "description": "",
  "bin": {
    "lumi": "./bin/index"
  },
  "main": "dist",
  "scripts": {
    "prepublish": "npm run build",
    "start": "ts-node ./src",
    "start:aws": "node --max-old-space-size=8192 -- node_modules/.bin/ts-node -e \"if (process.env.STAGE == 'prod') {process.env.NODE_ENV = 'production'}else{process.env.NODE_ENV = 'dev';};require('./src');\"",
    "start:watch": "nodemon",
    "dev": "npm run start -- START_SERVER",
    "echo": "npm run start -- echo test",
    "build": "tsc",
    "test:all": "npm test tests/**/*.test.ts",
    "deploy": "npm run build && git push",
    "lint": "tsc --noEmit && eslint \"./src/**/*.ts\" --fix",
    "test": "mocha --exit -r ts-node/register tests/helper.test.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^4.0.1",
    "chalk": "^2.4.2",
    "chokidar": "^3.4.0",
    "commander": "^5.0.0",
    "deepmerge": "^4.2.2",
    "diff": "^4.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "extract-zip": "^2.0.0",
    "form-data": "^3.0.0",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.0",
    "node-watch": "^0.6.3",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "slash": "^3.0.0",
    "socket.io-client": "^2.3.0",
    "tspath": "^1.3.7",
    "zip-local": "^0.3.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/diff": "^4.0.2",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.32",
    "@types/node-fetch": "^2.5.4",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "chai": "^4.2.0",
    "chai-url": "^1.0.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^6.2.3",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.8.3"
  },
  "_moduleAliases": {
    "@src": "src",
    "@api": "src/api",
    "@cli": "src/cli",
    "@common": "src/common",
    "@events": "src/events"
  }
